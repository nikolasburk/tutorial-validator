{
  "name": "@tutorial-validator/step-executor",
  "version": "0.1.0",
  "description": "Deterministic tutorial step executor",
  "type": "module",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "bin": {
    "tutorial-executor": "./dist/cli.js"
  },
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js"
    }
  },
  "scripts": {
    "build": "tsc",
    "dev": "tsx src/cli.ts",
    "typecheck": "tsc --noEmit",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:ui": "vitest --ui",
    "test:coverage": "vitest run --coverage"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "@types/dockerode": "^3.3.45",
    "@types/js-yaml": "^4.0.9",
    "@types/node": "^24.9.2",
    "tsx": "^4.20.6",
    "typescript": "^5.9.3",
    "vitest": "workspace:*"
  },
  "dependencies": {
    "dockerode": "^4.0.9"
  },
  "optionalDependencies": {
    "playwright": "^1.40.0"
  }
}
